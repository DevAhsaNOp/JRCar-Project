                            @*<a class="btn-compare" href="#"><i class="icon-flow-tree"></i> Compare</a>*@
                            <a class="btn-shortlist" href="#"><i class="icon-heart-o"></i> shortlist</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Single - Page Slider End -->
